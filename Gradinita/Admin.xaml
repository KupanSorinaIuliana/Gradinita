<Window x:Class="Gradinita.Admin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Gradinita"
        mc:Ignorable="d"
        Title="Admin">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="resources/1.jpeg"/>
        </Grid.Background>
        <DataGrid x:Name="tableAdmin" HorizontalAlignment="Center" Height="500" VerticalAlignment="Center" Width="1000" d:ItemsSource="{d:SampleData ItemCount=5}" RenderTransformOrigin="0.497,0.572" SelectionChanged="DataGrid_SelectionChanged" IsEnabled="False" Margin="20,20,20,20" FontSize="16">
            <DataGrid.Background>
                <ImageBrush ImageSource="/farm-bg-9-field.jpeg"/>
            </DataGrid.Background>
        </DataGrid>

    </Grid>
</Window>
