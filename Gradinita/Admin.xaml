<Window x:Class="Gradinita.Admin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Gradinita"
        mc:Ignorable="d"
        Title="Admin" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="resources/1.jpeg"/>
        </Grid.Background>
        <DataGrid x:Name="tableAdmin" HorizontalAlignment="Center" Height="318" VerticalAlignment="Center" Width="362" d:ItemsSource="{d:SampleData ItemCount=5}" RenderTransformOrigin="0.497,0.572" SelectionChanged="DataGrid_SelectionChanged" Opacity="0.8" IsEnabled="False"/>

    </Grid>
</Window>
